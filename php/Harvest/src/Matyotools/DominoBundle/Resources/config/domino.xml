<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="matyotools_domino.service.class">Matyotools\DominoBundle\Services\DominoService</parameter>
        <parameter key="matyotools_domino.user">null</parameter>
        <parameter key="matyotools_domino.password">null</parameter>
    </parameters>

    <services>
        <service id="domino" class="%matyotools_domino.service.class%">
            <argument key="user">%matyotools_domino.user%</argument>
            <argument key="password">%matyotools_domino.password%</argument>
        </service>
    </services>
</container>
